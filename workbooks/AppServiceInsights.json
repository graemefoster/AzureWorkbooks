                  {
                    "columnMatch": "over_recommended_apps",
                    "formatter": 22,
                    "formatOptions": {
                      "compositeBarSettings": {
                        "labelText": "[\"apps_label\"]",
                        "columnSettings": [
                          {
                            "columnName": "used_recommended_apps",
                            "color": "green"
                          },
                          {
